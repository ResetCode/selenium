<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.selenum.dao.UsDataStateDao">

	<select id="findByStateCode" resultType="com.selenum.model.UsDataState">
		select * from us_data_state
		where state_code = #{code} limit 1
	</select>
</mapper>
